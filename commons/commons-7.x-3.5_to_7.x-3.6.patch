+class FileEntityTestHelper extends DrupalWebTestCase {
+  protected $files = array();
+
+  function setUp() {
+    $modules = func_get_args();
+    if (isset($modules[0]) && is_array($modules[0])) {
+      $modules = $modules[0];
+    }
+    $modules[] = 'file_entity';
