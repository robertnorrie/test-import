+      $this->assertRenderedElement($element, '//*');
+
+      if (isset($info['thumbnail']) && $info['thumbnail']) {
+        $element = oembed_render_element('oembed_thumbnail', $url);
+        $this->assertRenderedElement($element, '//img');
+      }
+    }
