+
+    $this->query = db_select('d6_og_users_roles', 'ogur');
+    $this->query->join('role', 'r', 'ogur.rid = r.rid');
+    $this->query->join('og_role', 'ogr', 'r.name = ogr.name');
+
+    $this->query->fields('ogur', array('gid', 'uid'))
